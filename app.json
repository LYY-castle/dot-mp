{
  "pages": [
    "pages/index/index",
    "pages/product/index",
    "pages/mine/mine",
    "pages/shopping_cart/shopping-cart",
    "components/specification/specification",
    "components/navigator/navigator"
  ],
  "subpackages": [
    {
      "name": "pages_product",
      "root": "pages_product",
      "pages": [
        "search/search",
        "product-list/product-list",
        "product-detail/product-detail",
        "perchase/perchase"
      ]
    },
    {
      "name": "pages_mine",
      "root": "pages_mine",
      "pages": [
        "personal-info/personal-info",
        "personal-info/cropper/cropper",
        "add-shopping-money/add-shopping-money",
        "login/login"
      ]
    },
    {
      "name": "pages_order",
      "root": "pages_order",
      "pages": [
        "order-list/order-list",
        "order-detail/order-detail"
      ]
    }
  ],
  "preloadRule": {
    "pages/product/index": {
      "network": "all",
      "packages": [
        "pages_product"
      ]
    }
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "金小点",
    "navigationBarTextStyle": "black",
    "onReachBottomDistance": 50,
    "enablePullDownRefresh": true
  },
  "tabBar": {
    "color": "#B2AEB2",
    "selectedColor": "#f9ae08",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/img/product-list.png",
        "selectedIconPath": "static/img/product-list-active.png"
      },
      {
        "pagePath": "pages/product/index",
        "text": "分类",
        "iconPath": "static/img/sorts.png",
        "selectedIconPath": "static/img/active-sorts.png"
      },
      {
        "pagePath": "pages/shopping_cart/shopping-cart",
        "text": "购物车",
        "iconPath": "static/img/cart.png",
        "selectedIconPath": "static/img/active-cart.png"
      },
      {
        "pagePath": "pages/mine/mine",
        "text": "个人",
        "iconPath": "static/img/mine.png",
        "selectedIconPath": "static/img/mine-active.png"
      }
    ]
  },
  "sitemapLocation": "sitemap.json"
}
